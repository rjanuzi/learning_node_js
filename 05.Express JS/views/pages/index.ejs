<div class="container my-4">
  <div class="row text-center">
    <% topSpeakers.speakers.forEach(speaker => {%>
      <div class="col">
        <h2 class="my-5 h2"><%= speaker.name %></h2>
        <img
          class="rounded-circle"
          alt="100x100"
          src="images/<%= speaker.photo %>"
          data-holder-rendered="true"
        />
      </div>
    <% }) %>
  </div>
</div>